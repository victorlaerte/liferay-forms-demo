{
    "id": "lfr",
    "port": 8080,
    "memory": 4096,
    "cpu": 4,
    "dependencies": ["db"],
    "env": {
        "LIFERAY_WEB_PERIOD_SERVER_PERIOD_PROTOCOL": "https"
    },
    "volumes": {
        "osgi": "/liferay/osgi",
        "data": "/opt/liferay/data/"
    },
    "healthCheck": {
        "url": "localhost:8080",
        "startPeriod": 180,
        "timeout": 999999,
        "interval": 30,
        "retries": 3
    }
}